{
  "name": "moleculer-web",
  "version": "0.2.0",
  "description": "Official API Gateway service for Moleculer framework",
  "main": "index.js",
  "scripts": {
    "demo": "node examples/index.js",
    "demow": "nodemon examples/index.js",
    "bench": "node benchmark/index.js",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint --ext=.js src test",
    "deps": "npm-check -u",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "keywords": [
    "api-gateway",
    "http-server",
    "microservices",
    "microservice"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ice-services/moleculer-web.git"
  },
  "author": "Ice Services",
  "license": "MIT",
  "devDependencies": {
    "benchmarkify": "2.1.0",
    "coveralls": "2.13.1",
    "eslint": "3.19.0",
    "express": "4.15.2",
    "fakerator": "0.3.0",
    "jest": "20.0.0",
    "jest-cli": "20.0.0",
    "jsonwebtoken": "7.4.0",
    "lolex": "1.6.0",
    "nodemon": "1.11.0",
    "npm-check": "5.4.0",
    "supertest": "3.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./src",
    "roots": [
      "../test"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/services/"
    ]
  },
  "dependencies": {
    "body-parser": "1.17.1",
    "es6-error": "4.0.2",
    "isstream": "0.1.2",
    "lodash": "4.17.4",
    "moleculer": "0.7.1",
    "nanomatch": "1.1.1",
    "resumer": "0.0.0",
    "serve-static": "1.12.2"
  },
  "engines": {
    "node": ">= 6.x.x"
  }
}
