{
  "name": "moleculer-web",
  "version": "0.4.0",
  "description": "Official API Gateway service for Moleculer framework",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon examples/index.js",
    "demo": "node examples/index.js",
    "bench": "node benchmark/index.js",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint --ext=.js src test",
    "deps": "npm-check -u",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "keywords": [
    "api-gateway",
    "http-server",
    "microservices",
    "microservice",
    "moleculer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ice-services/moleculer-web.git"
  },
  "author": "Ice Services",
  "license": "MIT",
  "devDependencies": {
    "benchmarkify": "2.1.0",
    "coveralls": "2.13.1",
    "eslint": "4.3.0",
    "express": "4.15.3",
    "fakerator": "0.3.0",
    "jest": "20.0.4",
    "jest-cli": "20.0.4",
    "jsonwebtoken": "7.4.1",
    "lolex": "2.1.1",
    "mkdirp": "0.5.1",
    "moleculer": "0.8.3",
    "multer": "1.3.0",
    "nats": "0.7.20",
    "nodemon": "1.11.0",
    "npm-check": "5.4.4",
    "resumer": "0.0.0",
    "supertest": "3.0.0"
  },
  "peerDependencies": {
    "moleculer": ">= 0.8.0"
  },
  "dependencies": {
    "body-parser": "1.17.2",
    "es6-error": "4.0.2",
    "isstream": "0.1.2",
    "lodash": "4.17.4",
    "nanomatch": "1.2.0",
    "path-to-regexp": "1.7.0",
    "serve-static": "1.12.3"
  },
  "engines": {
    "node": ">= 6.x.x"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./src",
    "roots": [
      "../test"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/services/"
    ]
  }
}
